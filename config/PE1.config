!
version 15.2
service timestamps debug datetime msec
service timestamps log datetime msec
!
hostname PE1
!
boot-start-marker
boot-end-marker
!
!
!
no aaa new-model
no ip icmp rate-limit unreachable
ip cef
!
!
!
!
!
!
no ip domain lookup
no ipv6 cef
!
!
multilink bundle-name authenticated
!
!
!
!
!
!
!
!
!
ip tcp synwait-time 5
! 
!
!
!
!
!
!
!
!
!
!
!
interface Loopback0
 ip address 6.6.6.6 255.255.255.255
!
interface FastEthernet0/0
 ip address 20.0.0.1 255.0.0.0
 speed auto
 duplex auto
!
interface FastEthernet0/1
 ip address 21.0.0.1 255.0.0.0
 speed auto
 duplex auto
!
interface Ethernet1/0
 ip address 172.16.0.1 255.255.0.0
 duplex full
!
interface Ethernet1/1
 no ip address
 shutdown
 duplex full
!
interface Ethernet1/2
 no ip address
 shutdown
 duplex full
!
interface Ethernet1/3
 no ip address
 shutdown
 duplex full
!
router ospf 100
 redistribute bgp 400 subnets
 network 6.6.6.6 0.0.0.0 area 0
 network 20.0.0.0 0.0.0.255 area 0
 network 21.0.0.0 0.0.0.255 area 0
 network 172.16.0.0 0.0.255.255 area 0
!
router bgp 400
 bgp log-neighbor-changes
 neighbor 7.7.7.7 remote-as 400
 neighbor 7.7.7.7 update-source Loopback0
 neighbor 7.7.7.7 next-hop-self
 neighbor 20.0.0.2 remote-as 400
 neighbor 21.0.0.2 remote-as 400
 neighbor 172.16.0.2 remote-as 200
!
ip forward-protocol nd
!
!
no ip http server
no ip http secure-server
!
!
!
!
control-plane
!
!
line con 0
 exec-timeout 0 0
 privilege level 15
 logging synchronous
 stopbits 1
line aux 0
 exec-timeout 0 0
 privilege level 15
 logging synchronous
 stopbits 1
line vty 0 4
 login
!
!
end